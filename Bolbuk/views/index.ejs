                            <p>Free Pickup and Delivery Available</p>
                            
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Hero Section End -->

   

    <!-- Featured Section Begin -->
    <section class="featured spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title">
                        <h2>Featured Product</h2>
                    </div>
                    <div class="featured__controls">
                        <ul>
                            <li class="active" data-filter="*">All</li>
                            <li data-filter=".oranges">Snacks</li>
                            <li data-filter=".fresh-meat">Oil</li>
                            <li data-filter=".vegetables">Vegetables and Fruits</li>
                            <li data-filter=".fastfood">Flour products</li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="row featured__filter">
                <% data.forEach(function(product){ %>
                <div class="col-lg-3 col-md-4 col-sm-6 mix oranges fresh-meat" style="background:hsla(0, 5%, 96%, 0.842)">
                    <div  class="featured__item">
                        <div class="featured__item__pic set-bg" data-setbg=<%=product.image%>>
                            <ul class="featured__item__pic__hover">
                                <li><a href="#"><i class="fa fa-heart"></i></a></li>
                                <li><a href="#"><i class="fa fa-retweet"></i></a></li>
                                <li>
                                    <form action="/products/cart/<%=product._id%>" method="post">
                                       <button class="btn btn-info">Add to cart</button>
                                  </form>
                               </li>
                            </ul>
                            
                        </div>
                        <div class="featured__item__text">
                            <h6><a href="#"><%= product.name%></a></h6>
                            <h5>Â£<%= product.price%></h5>
                        </div>
                    </div>
                       
                </div>
                <% }); %>
