            </div>
            <%} else { %>
                <div class="humberger__menu__cart">
                    <ul>
                        
                        <li style="list-style:none"><a href="#"><i class="fa fa-shopping-bag"></i> <span><%=session.cart.totalQty%></span></a></li>
                    </ul>
                    <div class="header__cart__price">item: <span></span></div>
                </div>
